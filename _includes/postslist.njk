<ul>
  {% for blog in postslist %}
    <li>
        <a class="postlist__link" href="{{ blog.url | url }}">{{ blog.data.title }}</a><br>

      <time class="postlist__time" datetime="{{ blog.date | htmlDateString }}">{{ blog.date | readableDate }}</time><br><br>
     {# {% excerpt post %} #}
    </li>
  {% endfor %}
</ul>